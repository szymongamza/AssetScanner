<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:cv="clr-namespace:Camera.MAUI;assembly=Camera.MAUI"
             x:Class="AssetScanner.MainPage">

    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" >
            <Label Text="Asset Management" FontSize="24" HorizontalOptions="CenterAndExpand" Margin="0,0,0,20" />
            <Button Text="Stocktakings" Clicked="StocktakingsButton_Clicked" FontSize="18" Margin="0,0,0,20" />
            <Button Text="Asset Info" Clicked="AssetInfoButton_Clicked" FontSize="18" Margin="0,0,0,20" />
        </StackLayout>
    </ContentPage.Content>

</ContentPage>
